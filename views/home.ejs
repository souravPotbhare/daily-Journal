<%- include("partials/header") -%>

<h1>Home</h1>

<p> <%= startingContent %> </p>
<br>
<div>
  <% posts.forEach(function(post) { %>
    <h3> <%= post.title %> </h3>
    <p> <%= post.content.substring(0, 100) + "..." %>
        <a href="/posts/<%=(post.title)%>">Read More</a>
    </p>
  <% }); %>
</div>

<div class="btn btn-default">
  <a href="/compose">New Post</a>
</div>

<%- include("partials/footer") -%>
