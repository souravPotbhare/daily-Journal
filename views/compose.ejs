<%- include("partials/header") -%>

<h1>Compose</h1>

<form class="" action="/compose" method="post">
  <div class="form-group">
    <label for="">Title</label>
    <input class="form-control" type="text" name="postTitle" placeholder="Big things start with Small, like this TITLE" autocomplete="off">
  </div>
  <div class="form-group">
    <label for="">Post</label>
    <textarea rows="5" cols="30" class="form-control" name="postBody" autocomplete="off" ></textarea>
  </div>
  <button  class="btn btn-primary" type="submit" name="publish">Publish</button>
</form>

<%- include("partials/footer") -%>
